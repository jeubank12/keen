{
  "name": "keen",
  "private": true,
  "author": "Keen.IO <team@keen.io> (https://keen.io/)",
  "homepage": "https://github.com/keen/keen#readme",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "commit": "npx git-cz",
    "prettier": "prettier --write 'packages/**/*.{js,ts,tsx,json}'",
    "lint": "eslint --quiet --ext .ts --ext .tsx packages/",
    "create-version": "lerna version --conventional-commits",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage -w 1",
    "update-packages": "lernaupdate",
    "storybook": "start-storybook -p 3000 -s .storybook",
    "build-storybook": "build-storybook -c .storybook -s .storybook -o dist",
    "publish-storybook": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn prettier",
      "eslint --fix",
      "git add *"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@mdx-js/loader": "^1.5.7",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@storybook/theming": "^5.2.8",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/storybook-addon-jsx": "^7.0.1",
    "@types/webpack": "^4.4.35",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gh-pages": "^2.1.1",
    "git-cz": "^3.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "lerna": "^3.18.4",
    "lerna-update-wizard": "^0.17.8",
    "lint-staged": "^9.5.0",
    "lorem-ipsum": "^2.0.3",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "rimraf": "^3.0.0",
    "storybook": "^5.1.11",
    "storybook-addon-jsx": "^7.1.13",
    "terser-webpack-plugin": "^2.3.5",
    "to-string-loader": "^1.1.6",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "yarn": "^1.21.1"
  }
}
