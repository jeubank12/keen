{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "removeComments": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true
  }
}
